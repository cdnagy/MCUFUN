#include "printInfo.h"




void printInfo(void){
	if(app.printFlag == 1){
		printf("%s", app.printBuffer);
		memset(app.printBuffer, 0, sizeof(app.printBuffer));
		app.printFlag = 0;
	}
	HAL_GPIO_TogglePin(GPIOB, LD3_Pin);
}
